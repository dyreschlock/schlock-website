<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html t:type="blog/layoutblog" post="page"
    xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
    xmlns:c="tapestry-library:blog/content"
    xmlns:l="tapestry-library:blog/link"
    >

    <c:postdisplay post="page" cssClass="blogPost" />

    <div id="aboutMe">

        <div id="email">
            <div class="title">${message:email-title}</div>
            <div class="details">${message:website-email}</div>
        </div>
        <!--
        <div id="resume1">
            <div class="title">${message:resume-title}</div>
            <div class="details">
                <a href="${message:resume-software-url}" target="_blank">${message:resume-software}</a><br/>
            </div>
        </div>
        <div id="resume2">
            <div class="title">&nbsp;</div>
            <div class="details">
                <a href="${message:resume-teaching-url}" target="_blank">${message:resume-teaching}</a>
            </div>
        </div>
        -->
        <br class="clr" />
        <br class="clr" />

        <div id="internetLinks">
            <div class="title">${message:internetTitle}</div>
            <l:iconlink icon="rss" redirectUrl="rssRedirect" altTitleText="${message:rssTitle}" />
            <l:iconlink icon="twitter" redirectUrl="${message:twitterUrl}" altTitleText="${message:twitterTitle}" />
            <l:iconlink icon="discord" redirectUrl="${message:discordUrl}" altTitleText="${message:discordTitle}" />
            <l:iconlink icon="twitch" redirectUrl="${message:twitchUrl}" altTitleText="${message:twitchUrl}" />
            <l:iconlink icon="youtube" redirectUrl="${message:youtube1Url}" altTitleText="${message:youtube1Title}" />
            <l:iconlink icon="youtube" redirectUrl="${message:youtube2Url}" altTitleText="${message:youtube2Title}" />
            <l:iconlink icon="ebay" redirectUrl="${message:ebayUrl}" altTitleText="${message:ebayTitle}" />
        </div>
        <!--div id="gameLinks">
            <div class="title">${message:gamesTitle}</div>
            <l:iconlink icon="psn" redirectUrl="${message:psnUrl}" altTitleText="${message:psnTitle}" />
            <l:iconlink icon="xbox" redirectUrl="${message:xboxUrl}" altTitleText="${message:xboxTitle}" />
            <l:iconlink icon="steam" redirectUrl="${message:steamUrl}" altTitleText="${message:steamTitle}" />
        </div-->
        <div id="codeLinks">
            <div class="title">${message:codeTitle}</div>
            <l:iconlink icon="stack" redirectUrl="${message:stackUrl}" altTitleText="${message:stackTitle}" />
            <l:iconlink icon="github" redirectUrl="${message:githubUrl}" altTitleText="${message:githubTitle}" />
        </div>

        <br class="clr" />

    </div>

</html>