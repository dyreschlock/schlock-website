<t:container
     xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
    >

    <h2 class="date">${postDate}</h2>

    <div class="blogbody">

        <h3 class="title">${postTitle}</h3>

        <t:outputraw value="postBodyHTML" />

        <a name="more"></a>

        <span class="posted">Posted by dyre at ${postDate}<br/></span>

    </div>
</t:container>