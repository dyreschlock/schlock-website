<t:container
        xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
        xmlns:p="tapestry-library:apps/pokemon/rocket"
>

    <tr>
        <td class="rocket">${rocketBossPokemon.name}</td>
        <t:loop source="counterPokemon" value="currentCounterPokemon" index="currentIndex">

            <p:rocketcounterdisplay counterPokemon="currentCounterPokemon" number="currentIndexPlusOne" />

        </t:loop>
    </tr>

</t:container>